{
  "name": "@reamplify/webapp",
  "private": true,
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "start:dev": "node ./scripts/env dev react-app-rewired start",
    "build:dev": "node ./scripts/env dev react-app-rewired build",
    "build": "node ./scripts/ci react-app-rewired build",
    "start": "yarn run start:dev",
    "eject": "react-scripts eject",
    "lint:fix": "tsc --noemit && eslint \"./src/**\" --ext .ts,.tsx --fix",
    "manual:deploy:dev": "aws s3 sync --delete ./build s3://web.dev.us-east-1.reamplify.io --profile dev && aws cloudfront create-invalidation --distribution-id E17LGI6834QAT0 --paths \"/*\" --profile dev"
  },
  "dependencies": {
    "@apollo/client": "^3.5.9",
    "@aws-amplify/api": "^4.0.39",
    "@aws-amplify/auth": "^4.5.3",
    "@aws-amplify/core": "^4.5.3",
    "@aws-amplify/ui-react": "^2.16.1",
    "@reamplify/schema": "workspace:*",
    "@types/node": "^16.11.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "apollo-link-http": "^1.5.17",
    "apollo-link-timeout": "^4.0.0",
    "aws-amplify": "^4.3.21",
    "aws-appsync-auth-link": "^3.0.7",
    "aws-appsync-subscription-link": "^3.0.9",
    "cross-spawn": "^7.0.3",
    "cuid": "^2.1.8",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.11.0",
    "lexorank": "^1.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-app-rewired": "^2.2.1",
    "react-dev-utils": "^11.0.3",
    "react-dom": "^18.1.0",
    "react-is": "^18.1.0",
    "react-scripts": "^4.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.6.4",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/jest": "^26.0.10",
    "@types/prop-types": "^15.7.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "eslint-plugin-unused-imports": "^1.1.1",
    "jest": "^27.4.7",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "styled-components": "^5.3.5",
    "ts-jest": "^27.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
